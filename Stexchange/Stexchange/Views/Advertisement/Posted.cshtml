@{
    ViewData["Title"] = "Posted";
}

@{
    object verificationId;
    ViewContext.RouteData.Values.TryGetValue("id", out verificationId);
}

<form action="Verification/@(verificationId)" method="get">
    <h2>Gelukt!</h2>
    <div>
        <p>Je advertentie "@TempData["Title"]" is succesvol geplaatst.</p>
    </div>
</form>
<button >
    Maak nog een advertentie
</button>

